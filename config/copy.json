{
  "html": {
    "files": [{
      "expand": true,
      "dot": true,
      "cwd": "./",
      "src": ["*.html"],
      "dest": "dist"
    }]
  },
  "fonts": {
    "files": [{
      "expand": true,
      "dot": true,
      "cwd": "node_modules/font-awesome",
      "src": ["fonts/*.*"],
      "dest": "dist"
    }]
  }
}